{
  "name": "fanzee-labs",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "prepare": "node -e \"if (process.env.NODE_ENV !== 'production'){process.exit(1)} \" || husky",
    "lint": "eslint ./src",
    "format": "prettier --write --list-different . && eslint ./src --fix",
    "test": "vitest run",
    "commit": "git-cz",
    "check": "vue-tsc --noEmit"
  },
  "dependencies": {
    "@pinia/nuxt": "^0.9.0",
    "pinia": "^2.3.0",
    "nuxt": "^3.15.0",
    "vue": "latest",
    "vue-router": "latest"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.6.0",
    "@nuxt/eslint": "^0.7.2",
    "@nuxt/eslint-config": "^0.7.2",
    "commitizen": "^4.3.1",
    "eslint": "^9.16.0",
    "git-cz": "^4.9.0",
    "husky": "^9.1.6",
    "prettier": "^3.3.3",
    "sass": "^1.81.0",
    "typescript": "5.6.3",
    "vite-plugin-eslint2": "^5.0.3",
    "vitest": "^2.1.5",
    "vue-tsc": "^2.1.10"
  },
  "config": {
    "commitizen": {
      "path": "git-cz"
    }
  }
}
